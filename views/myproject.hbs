<html>
    <head>
        
        <link rel="stylesheet" href="/public/my project/myproject.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    
        
    </head>
    <body >
        
        <nav class="navbar navbar-expand-lg navbar-light fixed-top">
            <div class="container-fluid">
              <a class="navbar-brand me-5" href="#"><img src="/public/dw.png"></a>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="/">Home</a>
                  </li>
                  <li class="nav-item">
                      {{#if isLogin}}
                    <a class="nav-link" href="/myproject">My Project</a>
                    {{/if}}
                  </li>
                </ul>
                <div class="d-flex align-items-center justify-content-center mt-0">
                  {{#if isLogin}}
                    <a class = "register m-4" href="#">Hallo, {{user.nama}}</a>
                    <a class="login me-4" href="/logout">Logout</a>    
                {{else}}
                    <a href="/register" class="register me-4">Register</a>
                 <a href="/login" class="login me-4">Log in</a>
                {{/if}}
                 <a href="contact" class="btn-mycontact">My Contact</a>
                </div>
              </div>
            </div>
          </nav>
    <div class= "blog" style="
    height: 950px; background-color: rgb(243, 243, 243);
    ">
        <h2 style="
        text-align:center;
        padding-top: 200px;
        ">ADD MY PROJECT</h2>
        <form  action="/" method="POST" enctype="multipart/form-data" style="width: 400px; margin: auto; margin-top: 50px;">
            <div style="margin-top: 10px;">
            </div>

            <div style="margin-bottom: 20px;">
                <label  for="input-project" style="font-size: 23px; font-weight: 700px;"> Project Name</label>
                <input
                    name="title"
                    type="text"
                    id="input-project"
                    style="
                        width: 100%;
                        padding: 14px;
                        border: 1px solid rgb(206, 206, 206);
                        margin-top: 2px;
                        box-shadow: 0px 2px 2px;
                        ">
            </div>
            <div class= "date">
            <div class= "right-input" style="margin-bottom: 20px;">
                <label  for="startdate" style="font-size: 23px; font-weight: 700px;">Start Date</label>
                <input
                    name="startdate"
                    type="date"
                    id="startdate"
                    style="
                        width: 100%;
                        padding: 14px;
                        border: 1px solid rgb(206, 206, 206);
                        margin-top: 2px;
                        box-shadow: 0px 2px 4px;
                        
                        ">
                        </div>
            
        
        <div class="left-input" style="margin-bottom: 20px;">
            <label  for="enddate" style="font-size: 23px; font-weight: 700px;">End Date</label>
            <input
                name="enddate"
                type="date"
                id="enddate"
                style="
                    width: 100%;
                    padding: 14px;
                    border: 1px solid rgb(206, 206, 206);
                    margin-top: 2px;
                    box-shadow: 0px 2px 4px;
                    
                    ">
        </div>
        </div>

        
    

    <div style="margin-bottom: 20px;">
        <label  for="Description" style="font-size: 23px; font-weight: 700px;">Description</label>
        <textarea
            name="description"
            type="text area"
            id="Description"
            style="
                width: 100%;
                height: 90px;
                padding: 14px;
                border: 1px solid rgb(233, 233, 233);
                margin-top: 2px;
                box-shadow: 0px 2px 4px;
                "></textarea>
    </div>
    <h3>Technologies</h3>
    <div style="display: flex";>
            
            <div style="flex: 50%;">
               
                <input 
                name="techno"
                id="Node-js" 
                type="checkbox"
                value="node-js"
                class="checkbox"><label
                style="
                margin-left: 30px;"
                for="Node JS">Node JS</label>
                <br>
                <input 
                name="techno"
                id="React JS" 
                type="checkbox"
                Value= "react"
                class="checkbox"><label 
                style="
                margin-left: 30px;"
                For="React JS">React JS</label>
            </div>
            
            <div style="flex: 50%">        
                <input 
                name="techno"
                id="Next JS" 
                type="checkbox" 
                value="next-js"
                class="checkbox"><label 
                style="
                margin-left: 30px;"
                For="Next JS">Next JS</label>
                <br>
                <input 
                name="techno"
                id="TypeScript" 
                type="checkbox"
                value="typescript"
                class="checkbox"
                ><label 
                style="
                margin-left: 30px;"
                for="TypeScript">TypeScript</label>
            </div>
            
    </div>
            <h3>Upload Image</h3>
            <div  for="input-blog-image" style="
            border: 1px solid rgb(206, 206, 206); 
            background-color: white; 
            border-radius: 6px;
            box-shadow: 0px 2px 2px;">
                <label class="image" for="input-blog-image" 
                style="
                border: 1px solid rgb(206, 206, 206);
                color:rgb(19, 18, 18); 
                background-color:rgb(224, 224, 224);
                padding:10px;
                border-radius: 6px;
                box-shadow: 0px 2px 2px;

                ">Choose</label>
                <input name="image" id="input-blog-image" type="file" style="
                background-color: white;" hidden/>
                <img src="/public/my project/clip.png"
                style="height: 28px; width: 28px; margin-top: 12px; float: right;
                ">
                
            </div>

    <button 
        type="submit"
        style="
        background-color:black;
        float:right;
        color:white;
        border-radius: 20px;
        padding-top:7px;
        padding-right: 9px;
        padding-left: 9px;
        padding-bottom: 7px;
        margin-top:10px;
        width: 120px;
        ;">submit</button>
        </form>

        
        
    </div>




    <script src="/public/my project/myproject.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
          
    </body>
    
</html>
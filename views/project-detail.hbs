<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="../public/myproject/blog-detail.css">
    <link rel="stylesheet" href="../public/my project/myproject.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://code.iconify.design/2/2.2.1/iconify.min.js"></script>
    <title></title>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light fixed-top">
        <div class="container-fluid">
          <a class="navbar-brand me-5" href="#"><img src="../public/dw.png"></a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="/">Home</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/myproject">My Project</a>
              </li>
            </ul>
            <div class="d-flex align-items-center justify-content-center mt-0">
{{#if isLogin}}
                    <a class = "register m-4" href="#">Hallo, {{user.nama}}</a>
                    <a class="login me-4" href="/logout">Logout</a>    
                {{else}}
                    <a href="/register" class="register me-4">Register</a>
                 <a href="/login" class="login me-4">Log in</a>
                {{/if}}
             <a href="/contact" class="btn-mycontact">My Contact</a>
            </div>
          </div>
        </div>
      </nav>
<div style="
margin:40px;
margin-top: 40px;">

<h1 
style="
text-align: center;
margin-top: 100px;"> {{project.title}} </h1>

  <div class="row min-width: 0 ">
    <div class="col-7">
      <img class="img-fluid"  src="/uploads/{{this.image}}" alt="">
    </div>
    <div class="col-5 min">
      <script src="https://kit.fontawesome.com/a91abe3c0a.js" crossorigin="anonymous"></script>
     <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
      <div class="container-sm">
<div class="container float-sm-start">
<div class="row ">
  <h6 style="padding-left:50px;">Technologies</h6>
<span style="padding-left:50px;"class="iconify col-3 w3-xlarge mt-3" data-icon="bxs:calculator"></span><span style="padding-top:15px;" class="col-9">{{this.startDate}} - {{this.endDate}}</span>

<span style="padding-left:50px;"class="iconify col-3 w3-xlarge mt-3" data-icon="akar-icons:clock"></span><span style="padding-top:15px;" class="col-9">{{this.durasi}}</span>
      
    </div>

<!-- node js : akar-icons:node-fill, react js : akar-icons:react-fill , Javascript: akar-icons:javascript-fill , socket : cib:socket-io -->

<div style="margin-top: 50px;"class="row">
  <h6 style="padding-left:50px;">Technologies</h6>
  {{#each project.technologies}}
<span style="padding-left:50px;"class="iconify col-3 w3-xlarge mt-3" data-icon="cib:{{this}}"></span><span style="padding-top:15px;" class="col-3">{{this}}</span>
  {{/each}}
      
    </div>
  </div>
</div>
      </div>
    </div>
  </div>

  <p style="margin-top: 450px;
  ">{{project.description}}</p>


      
</div>
</body>
</html>
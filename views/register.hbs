    <div class= "blog" style="
    height: 950px; background-color: rgb(243, 243, 243);
    ">
        <h2 style="
        text-align:center;
        padding-top: 200px;
        ">REGISTER</h2>

    {{#if messages.error}}
          <div class='alert alert-danger' role='alert'>
            {{messages.error}}
          </div>
        {{/if}}

        <form  action="/register" method="POST" style="width: 400px; margin: auto; margin-top: 50px;">
            <div style="margin-top: 10px;">
            </div>

            <div style="margin-bottom: 20px;">
                <label  for="input-name" style="font-size: 23px; font-weight: 700px;">Name</label>
                <input
                    name="nama"
                    type="text"
                    id="input-name"
                    style="
                        width: 100%;
                        padding: 14px;
                        border: 1px solid rgb(206, 206, 206);
                        margin-top: 2px;
                        box-shadow: 0px 2px 2px;
                        ">
            </div>

             <div style="margin-bottom: 20px;">
                <label  for="input-email" style="font-size: 23px; font-weight: 700px;"> Email</label>
                <input
                    name="email"
                    type="email"
                    id="email"
                    style="
                        width: 100%;
                        padding: 14px;
                        border: 1px solid rgb(206, 206, 206);
                        margin-top: 2px;
                        box-shadow: 0px 2px 2px;
                        ">
            </div>

 <div style="margin-bottom: 20px;">
                <label  for="password" style="font-size: 23px; font-weight: 700px;"> Password</label>
                <input
                    name="password"
                    type="password"
                    id="input-password"
                    style="
                        width: 100%;
                        padding: 14px;
                        border: 1px solid rgb(206, 206, 206);
                        margin-top: 2px;
                        box-shadow: 0px 2px 2px;
                        ">
            </div>
        
    


    <button 
        type="submit"
        style="
        background-color:black;
        float:right;
        color:white;
        border-radius: 20px;
        padding-top:7px;
        padding-right: 9px;
        padding-left: 9px;
        padding-bottom: 7px;
        margin-top:10px;
        width: 120px;
        ;">Register</button>
        </form>

        
        
    </div>




    <script src="/public/my project/myproject.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
          
